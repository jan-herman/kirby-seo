{
    "name": "jan-herman/kirby-seo",
    "description": "SEO plugin for Kirby CMS",
    "version": "1.0.0",
    "type": "kirby-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jan-herman/kirby-seo",
    "authors": [
        {
            "name": "Jan Herman",
            "email": "mail@janherman.com",
            "homepage": "https://janherman.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "getkirby/composer-installer": "^1.2",
        "jan-herman/kirby-utils": "^2.8",
        "johannschopplich/kirby-serp-preview": "^1.3",
        "spatie/schema-org": "^3.23"
    },
    "extra": {
        "installer-name": "seo"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "JanHerman\\Seo\\": "lib/"
        }
    }
}